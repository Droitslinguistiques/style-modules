  </info>
  <locale>
    <terms>
      <term name="article">
        <single>article</single>
        <multiple>articles</multiple>
      </term>
      <term name="article" form="short">
        <single>art.</single>
        <multiple>arts.</multiple>
      </term>
      <term form="symbol" name="chapter">ch.</term>
      <term form="short" name="number">no.</term>
      <!-- ordinals -->
      <term name="ordinal">th</term>
      <term name="ordinal-01">st</term>
      <term name="ordinal-02">d</term>
      <term name="ordinal-03">d</term>
      <term name="ordinal-11">th</term>
      <term name="ordinal-12">th</term>
      <term name="ordinal-13">th</term>
      <!-- id -->
      <term name="ibid" form="short">id.</term>
      <term name="ibid" form="long">id.</term>
    </terms>
    <date form="text">
      <date-part name="month" form="short" suffix=" "/>
      <date-part name="day" form="numeric" suffix=", "/>
      <date-part name="year"/>
    </date>
  </locale>
  <macro name="juris-pretitle">
    <choose>
      <if condition-id="z4283" type="legal_case">
        <choose>
          <if condition-id="z4284" variable="document-name">
            <group delimiter=" ">
              <text variable="document-name"/>
              <text macro="juris-locator"/>
            </group>
          </if>
        </choose>
      </if>
      <else-if condition-id="z4285" type="report">
        <choose>
          <if condition-id="z4286">
            <conditions match="all">
              <condition match="all" type="report"/>
              <condition match="any" variable="title committee"/>
              <condition match="all" variable="author"/>
            </conditions>
            <names variable="author">
              <name/>
              <institution institution-parts="short"/>
            </names>
          </if>
          <else-if condition-id="z4287" match="none" variable="call-number">
            <names variable="authority">
              <name/>
              <institution institution-parts="short" delimiter=", "/>
            </names>
          </else-if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <macro name="juris-title">
    <choose>
      <if condition-id="z4288" type="regulation">
        <choose>
          <if condition-id="z4289">
            <conditions match="all">
              <condition match="all" variable="authority genre volume container-title page"/>
              <condition match="none" variable="gazette-ref"/>
            </conditions>
            <names variable="dummy">
              <name/>
              <institution institution-parts="short"/>
              <substitute>
                <group>
                  <choose>
                    <if position="near-note">
                      <text variable="title" form="short"/>
                    </if>
                    <else>
                      <text variable="title"/>
                    </else>
                  </choose>
                </group>
              </substitute>
            </names>
          </if>
          <else-if condition-id="z4290">
            <conditions match="all">
              <condition match="all" variable="authority" has-subunit="authority"/>
              <condition match="none" variable="genre"/>
            </conditions>
            <group delimiter=" ">
              <names variable="authority">
                <name/>
                <institution institution-parts="short" stop-last="1"/>
                <substitute>
                  <text variable="genre"/>
                </substitute>
              </names>
              <number variable="number"/>
            </group>
          </else-if>
          <else-if condition-id="z4291">
            <conditions match="all">
              <condition match="all" variable="container-title gazette-flag"/>
              <condition match="none" variable="number"/>
            </conditions>
          </else-if>
          <else condition-id="z4292">
            <group delimiter=" ">
              <group delimiter=" of ">
                <names variable="dummy">
                  <name/>
                  <substitute>
                    <group>
                      <choose>
                        <if position="near-note">
                          <text variable="title" form="short"/>
                        </if>
                        <else>
                          <text variable="title"/>
                        </else>
                      </choose>
                    </group>
                  </substitute>
                </names>
                <choose>
                  <if condition-id="z4293" variable="title">
                    <number variable="collection-number"/>
                  </if>
                </choose>
              </group>
              <choose>
                <if condition-id="z4294" variable="title">
                  <group delimiter=" ">
                    <label variable="number" form="short" text-case="capitalize-first"/>
                    <number variable="number"/>
                  </group>
                </if>
              </choose>
            </group>
          </else>
        </choose>
      </if>

      <!-- juris-title -->
      <else-if condition-id="z4295" match="any" type="legislation">
        <choose>
          <if condition-id="z4296">
            <conditions match="all">
              <condition match="all" variable="gazette-flag container-title volume page publication-date"/>
              <condition match="none" variable="issued"/>
            </conditions>
          </if>
          <else-if condition-id="z4297" match="all" variable="container-title collection-number gazette-flag">
            <group delimiter=" of ">
              <names variable="dummy">
                <name/>
                <substitute>
                  <group>
                    <choose>
                      <if position="near-note">
                        <text variable="title" form="short"/>
                      </if>
                      <else>
                        <text variable="title"/>
                      </else>
                    </choose>
                  </group>
                </substitute>
              </names>
              <choose>
                <if condition-id="z4298" variable="title">
                  <number variable="collection-number"/>
                </if>
              </choose>
            </group>
          </else-if>
          <else-if condition-id="z4299">
            <conditions match="all">
              <condition match="all" variable="container-title volume publisher publication-date"/>
            </conditions>
            <names variable="dummy">
              <name/>
              <institution institution-parts="short"/>
              <substitute>
                <group>
                  <choose>
                    <if position="near-note">
                      <text variable="title" form="short"/>
                    </if>
                    <else>
                      <text variable="title"/>
                    </else>
                  </choose>
                </group>
              </substitute>
            </names>
          </else-if>
          <else-if condition-id="z4300">
            <conditions match="all">
              <condition match="all" variable="issued"/>
            </conditions>
            <names variable="dummy">
              <name/>
              <institution institution-parts="short"/>
              <substitute>
                <group>
                  <choose>
                    <if position="near-note">
                      <text variable="title" form="short"/>
                    </if>
                    <else>
                      <text variable="title"/>
                    </else>
                  </choose>
                </group>
              </substitute>
            </names>
          </else-if>
        </choose>
      </else-if>
      
      <else-if condition-id="z4301" type="legal_case">
        <group parallel-first="title">
          <names variable="dummy">
            <name/>
            <substitute>
              <group>
                <choose>
                  <if position="near-note">
                    <text variable="title" form="short"/>
                  </if>
                  <else>
                    <text variable="title"/>
                  </else>
                </choose>
              </group>
            </substitute>
          </names>
        </group>
      </else-if>
      <else-if condition-id="z4302" type="bill">
        <choose>
          <if condition-id="z4303" variable="event"/>
          <else-if match="all" type="bill" position="near-note">
            <names variable="dummy">
              <name/>
              <institution/>
              <substitute>
                <number variable="number"/>
              </substitute>
            </names>
          </else-if>
          <else-if condition-id="z4304" variable="title">
            <choose>
              <if position="near-note">
                <text variable="title" form="short"/>
              </if>
              <else>
                <text variable="title"/>
              </else>
            </choose>
          </else-if>
        </choose>
      </else-if>

      
      <else-if condition-id="z4305" match="all" type="hearing">
        <group delimiter=": ">
          <group delimiter=" of ">
            <choose>
              <if position="near-note">
                <text variable="title" form="short"/>
              </if>
              <else>
                <text variable="title"/>
              </else>
            </choose>
            <choose>
              <if condition-id="z4306" match="all" variable="title number">
                <date variable="issued" form="text" date-parts="year"/>
              </if>
            </choose>
          </group>
          <group delimiter=" ">
            <text value="Hearing"/>
            <group delimiter=" ">
              <text value="on"/>
              <number variable="number"/>
            </group>
            <group delimiter=" ">
              <text value="Before the"/>
              <text variable="section"/>
            </group>
          </group>
        </group>
      </else-if>
      <else-if condition-id="z4307" type="report">
        <choose>
          <if condition-id="z4308">
            <conditions match="all">
              <condition match="any" variable="title committee"/>
            </conditions>
            <group delimiter=", ">
              <choose>
                <if condition-id="z4309" match="all" variable="author"/>
                <else-if condition-id="z4310" match="all" variable="committee">
                  <names variable="committee">
                    <name/>
                    <institution institution-parts="short"/>
                  </names>
                </else-if>
              </choose>
              <choose>
                <if condition-id="z4311">
                  <conditions match="all">
                    <condition match="none" variable="number"/>
                    <condition match="all" variable="collection-number"/>
                  </conditions>
                  <group delimiter=" ">
                    <number variable="collection-number" form="ordinal"/>
                    <names variable="authority">
                      <name/>
                      <institution institution-parts="short" use-last="1"/>
                    </names>
                  </group>
                </if>
              </choose>
              <choose>
                <if position="near-note">
                  <text variable="title" form="short"/>
                </if>
                <else>
                  <text variable="title"/>
                </else>
              </choose>
            </group>
          </if>
          <else-if condition-id="z4312" variable="number">
            <group delimiter=" ">
              <names variable="authority">
                <name/>
                <institution stop-last="1" institution-parts="short"/>
              </names>
              <choose>
                <if condition-id="z4313" match="any" variable="genre">
                  <text variable="genre" form="short"/>
                </if>
                <else condition-id="z4314">
                  <text value="Rep."/>
                </else>
              </choose>
              <group delimiter=" ">
                <label variable="number" form="symbol" text-case="capitalize-first"/>
                <number variable="number"/>
              </group>
            </group>
          </else-if>
          <else-if condition-id="z4315" match="none" variable="number">
            <group delimiter=", ">
              <group delimiter=" ">
                <number variable="collection-number" form="ordinal"/>
                <names variable="authority">
                  <name/>
                  <institution institution-parts="short" use-last="1"/>
                </names>
              </group>
              <choose>
                <if position="near-note">
                  <text variable="title" form="short"/>
                </if>
                <else>
                  <text variable="title"/>
                </else>
              </choose>
            </group>
          </else-if>
        </choose>
      </else-if>
      <else-if condition-id="z4316" type="patent">
        <names variable="dummy">
          <name/>
          <substitute>
            <text variable="title"/>
          </substitute>
        </names>
      </else-if>
    </choose>
  </macro>
  <macro name="juris-main">
    <choose>
      <if condition-id="z4317" match="any" type="regulation">
        <group delimiter=" ">
          <!-- renders only if title not blocked by cs:substitute in juris-title -->
          <names variable="dummy">
            <name/>
            <substitute>
              <group>
                <choose>
                  <if position="near-note">
                    <text variable="title" form="short"/>
                  </if>
                  <else>
                    <text variable="title"/>
                  </else>
                </choose>
              </group>
            </substitute>
          </names>
          <choose>
            <if condition-id="z4318">
              <conditions match="all">
                <condition match="all" variable="authority genre volume container-title page"/>
                <condition match="none" variable="gazette-ref"/>
              </conditions>
              <!--  group><text value="[LegReg OK1]"/></group -->
              <group delimiter=", ">
                <group delimiter=" ">
                  <text variable="genre"/>
                  <label variable="number" form="short" text-case="capitalize-first"/>
                  <number variable="number"/>
                </group>
                <group delimiter=" ">
                  <number variable="volume"/>
                  <text variable="container-title"/>
                  <number variable="page"/>
                </group>
              </group>
            </if>
            <else-if condition-id="z4319">
              <conditions match="all">
                <condition match="all" variable="genre volume container-title page"/>
                <condition match="none" variable="gazette-ref authority"/>
              </conditions>
              <!-- group><text value="[LegReg OK2]"/></group -->
              <group delimiter=", ">
                <group delimiter=" ">
                  <text variable="genre"/>
                  <number variable="number"/>
                </group>
                <group delimiter=" ">
                  <number variable="volume"/>
                  <text variable="container-title"/>
                  <number variable="page"/>
                </group>
              </group>
            </else-if>
            <else-if condition-id="z4320" match="all" variable="gazette-flag">
              <choose>
                <if condition-id="z4321">
                  <conditions match="all">
                    <condition match="none" variable="container-title"/>
                    <condition match="all" variable="number"/>
                  </conditions>
                  <group delimiter=" ">
                    <choose>
                      <if condition-id="z4322" variable="collection-number">
                        <number variable="collection-number"/>
                      </if>
                    </choose>
                    <text variable="jurisdiction" form="short"/>
                    <text value="Pub. L."/>
                    <number variable="number"/>
                  </group>
                </if>
                <else-if condition-id="z4323">
                  <conditions match="all">
                    <condition match="all" variable="container-title title"/>
                    <condition match="none" variable="number"/>
                  </conditions>
                  <group delimiter=" ">
                    <choose>
                      <if position="near-note">
                        <text variable="title" form="short"/>
                      </if>
                      <else>
                        <text variable="title"/>
                      </else>
                    </choose>
                    <text macro="juris-locator" label-form="symbol" suffix=","/>
                  </group>
                </else-if>
                <else-if condition-id="z4324" match="all" variable="container-title title">
                  <choose>
                    <if position="near-note">
                      <text variable="title" form="short"/>
                    </if>
                    <else>
                      <text variable="title"/>
                    </else>
                  </choose>
                </else-if>
              </choose>
              <choose>
                <if condition-id="z4325" match="any" variable="title container-title">
                  <choose>
                    <if condition-id="z4326" match="all" variable="number" number="number" type="legislation">
                      <group delimiter=" ">
                        <group delimiter=" " suffix=",">
                          <text value="Pub. L. No."/>
                          <number variable="number"/>
                        </group>
                        <text macro="juris-locator" label-form="symbol" suffix=","/>
                      </group>
                    </if>
                    <else-if condition-id="z4327" variable="number" type="legislation">
                      <group delimiter=" " suffix=",">
                        <label variable="number" form="symbol"/>
                        <number variable="number"/>
                      </group>
                    </else-if>
                  </choose>
                  <choose>
                    <if condition-id="z4328">
                      <conditions match="all">
                        <condition variable="collection-number"/>
                        <condition match="none" variable="number"/>
                      </conditions>
                      <group delimiter=", ">
                        <group delimiter=" ">
                          <number variable="collection-number"/>
                          <text variable="container-title" form="short"/>
                          <group delimiter=" ">
                            <label variable="number" form="short" text-case="capitalize-first"/>
                            <number variable="volume"/>
                          </group>
                        </group>
                        <number variable="page"/>
                      </group>
                    </if>
                  </choose>
                </if>
              </choose>
              <choose>
                <if condition-id="z4329">
                  <conditions match="any">
                    <condition match="none" variable="collection-number"/>
                    <condition match="all" variable="number"/>
                  </conditions>
                  <!-- weird outside-parens elements for statutes at large -->
                  <group delimiter=" ">
                    <number variable="volume"/>
                    <text variable="container-title" form="short"/>
                    <group delimiter=", ">
                      <number variable="page"/>
                      <text variable="locator-extra"/>
                    </group>
                  </group>
                </if>
              </choose>
            </else-if>
            <else condition-id="z4330">
              <group delimiter=" ">
                <group delimiter=", ">
                  <group delimiter=" ">
                    <choose>
                      <if condition-id="z4331">
                        <conditions match="any">
                          <condition is-numeric="volume"/>
                          <condition match="none" variable="volume"/>
                        </conditions>
                        <group delimiter="-">
                          <number variable="collection-number"/>
                          <number variable="volume"/>
                        </group>
                      </if>
                    </choose>
                    <text variable="container-title" form="short"/>
                    <choose>
                      <if condition-id="z4332" match="none" variable="title">
                        <names variable="authority">
                          <name/>
                          <institution institution-parts="short"/>
                        </names>
                      </if>
                    </choose>
                    <choose>
                      <if condition-id="z4333">
                        <conditions match="all">
                          <condition match="none" is-numeric="volume"/>
                          <condition type="regulation"/>
                        </conditions>
                        <number variable="volume"/>
                      </if>
                    </choose>
                  </group>
                  <choose>
                    <if condition-id="z4334">
                      <conditions match="all">
                        <condition match="none" is-numeric="volume"/>
                        <condition type="legislation"/>
                      </conditions>
                      <number variable="volume"/>
                    </if>
                  </choose>
                </group>
                <number variable="page"/>
              </group>
            </else>
          </choose>
        </group>
      </if>
      <else-if condition-id="z4335" match="any" type="legislation">
        <group delimiter=" ">
          <names variable="dummy">
            <name/>
            <substitute>
              <group>
                <choose>
                  <if position="near-note">
                    <text variable="title" form="short"/>
                  </if>
                  <else>
                    <text variable="title"/>
                    </else>
                </choose>
              </group>
            </substitute>
          </names>
          <choose>
            <if condition-id="z4336">
              <conditions match="all">
                <condition type="regulation"/>
                <condition match="all" variable="authority genre volume container-title page"/>
                <condition match="none" variable="gazette-ref"/>
              </conditions>
              <!--  group><text value="[LegReg OK1]"/></group -->
              <group delimiter=", ">
                <group delimiter=" ">
                  <text variable="genre"/>
                  <label variable="number" form="short" text-case="capitalize-first"/>
                  <number variable="number"/>
                </group>
                <group delimiter=" ">
                  <number variable="volume"/>
                  <text variable="container-title"/>
                  <number variable="page"/>
                </group>
              </group>
            </if>
            <else-if condition-id="z4337">
              <conditions match="all">
                <condition type="regulation"/>
                <condition match="all" variable="genre volume container-title page"/>
                <condition match="none" variable="gazette-ref authority"/>
              </conditions>
              <!-- group><text value="[LegReg OK2]"/></group -->
              <group delimiter=", ">
                <group delimiter=" ">
                  <text variable="genre"/>
                  <number variable="number"/>
                </group>
                <group delimiter=" ">
                  <number variable="volume"/>
                  <text variable="container-title"/>
                  <number variable="page"/>
                </group>
              </group>
            </else-if>
            <else-if condition-id="z4338" match="all" variable="gazette-flag">
              <choose>
                <if condition-id="z4339">
                  <conditions match="all">
                    <condition match="none" variable="container-title"/>
                    <condition match="all" variable="number"/>
                  </conditions>
                  <group delimiter=" ">
                    <choose>
                      <if condition-id="z4340" variable="collection-number">
                        <number variable="collection-number"/>
                      </if>
                    </choose>
                    <text variable="jurisdiction" form="short"/>
                    <text value="Pub. L."/>
                    <number variable="number"/>
                  </group>
                </if>
                <else-if condition-id="z4341">
                  <conditions match="all">
                    <condition match="all" variable="container-title title"/>
                    <condition match="none" variable="number"/>
                  </conditions>
                  <group delimiter=" ">
                    <choose>
                      <if position="near-note">
                        <text variable="title" form="short"/>
                      </if>
                      <else>
                        <text variable="title"/>
                      </else>
                    </choose>
                    <text macro="juris-locator" label-form="symbol" suffix=","/>
                  </group>
                </else-if>
                <else-if condition-id="z4342" match="all" variable="container-title title">
                  <choose>
                    <if position="near-note">
                      <text variable="title" form="short"/>
                    </if>
                    <else>
                      <text variable="title"/>
                    </else>
                  </choose>
                </else-if>
              </choose>
              <choose>
                <if condition-id="z4343" match="any" variable="title container-title">
                  <choose>
                    <if condition-id="z4344" match="all" variable="number" number="number" type="legislation">
                      <group delimiter=" ">
                        <group delimiter=" " suffix=",">
                          <text value="Pub. L. No."/>
                          <number variable="number"/>
                        </group>
                        <text macro="juris-locator" label-form="symbol" suffix=","/>
                      </group>
                    </if>
                    <else-if condition-id="z4345" variable="number" type="legislation">
                      <group delimiter=" " suffix=",">
                        <label variable="number" form="symbol"/>
                        <number variable="number"/>
                      </group>
                    </else-if>
                  </choose>
                  <choose>
                    <if condition-id="z4346">
                      <conditions match="all">
                        <condition variable="collection-number"/>
                        <condition match="none" variable="number"/>
                      </conditions>
                      <group delimiter=", ">
                        <group delimiter=" ">
                          <number variable="collection-number"/>
                          <text variable="container-title" form="short"/>
                          <group delimiter=" ">
                            <label variable="number" form="short" text-case="capitalize-first"/>
                            <number variable="volume"/>
                          </group>
                        </group>
                        <number variable="page"/>
                      </group>
                    </if>
                  </choose>
                </if>
              </choose>
              <choose>
                <if condition-id="z4347">
                  <conditions match="any">
                    <condition match="none" variable="collection-number"/>
                    <condition match="all" variable="number"/>
                  </conditions>
                  <!-- weird outside-parens elements for statutes at large -->
                  <group delimiter=" ">
                    <number variable="volume"/>
                    <text variable="container-title" form="short"/>
                    <group delimiter=", ">
                      <number variable="page"/>
                      <text variable="locator-extra"/>
                    </group>
                  </group>
                </if>
              </choose>
            </else-if>
            <else condition-id="z4348">
              <group delimiter=" ">
                <group delimiter=", ">
                  <group delimiter=" ">
                    <choose>
                      <if condition-id="z4349">
                        <conditions match="any">
                          <condition is-numeric="volume"/>
                          <condition match="none" variable="volume"/>
                        </conditions>
                        <group delimiter="-">
                          <number variable="collection-number"/>
                          <number variable="volume"/>
                        </group>
                      </if>
                    </choose>
                    <text variable="container-title" form="short"/>
                    <choose>
                      <if condition-id="z4350" match="none" variable="title">
                        <names variable="authority">
                          <name/>
                          <institution institution-parts="short"/>
                        </names>
                      </if>
                    </choose>
                    <choose>
                      <if condition-id="z4351">
                        <conditions match="all">
                          <condition match="none" is-numeric="volume"/>
                          <condition type="regulation"/>
                        </conditions>
                        <number variable="volume"/>
                      </if>
                    </choose>
                  </group>
                  <choose>
                    <if condition-id="z4352">
                      <conditions match="all">
                        <condition match="none" is-numeric="volume"/>
                        <condition type="legislation"/>
                      </conditions>
                      <number variable="volume"/>
                    </if>
                  </choose>
                </group>
                <number variable="page"/>
              </group>
