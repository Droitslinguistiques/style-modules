                    <else condition-id="z4433">
                      <group parallel-last="issued">
                        <date variable="issued" form="text" date-parts="year"/>
                      </group>
                    </else>
                  </choose>
                </else-if>
                <else-if condition-id="z4434" match="all" variable="container-title archive_location">
                  <date variable="issued" form="text" date-parts="year-month-day"/>
                </else-if>
                <else-if condition-id="z4435">
                  <conditions match="all">
                    <condition match="all" variable="title admin-flag authority number"/>
                    <condition match="none" variable="archive_location page container-title"/>
                  </conditions>
                    <date variable="issued" form="text" date-parts="year-month-day"/>
                </else-if>
                <else-if condition-id="z4436">
                  <conditions match="all">
                    <condition match="all" variable="archive"/>
                    <condition match="none" variable="container-title"/>
                  </conditions>
                  <group delimiter=", ">
                    <choose>
                      <if condition-id="z4437" variable="admin-flag publisher" match="all"> 
                        <group delimiter=" ">
                          <label variable="number" form="short" text-case="capitalize-first"/>
                          <number variable="number"/>
                        </group>
                      </if>
                    </choose>
                    <date variable="issued" form="text" date-parts="year-month-day"/>
                  </group>
                </else-if>
                <else-if condition-id="z4438" match="none" variable="admin-flag">
                  <date variable="issued" form="text" date-parts="year-month-day"/>
                </else-if>
                <else-if condition-id="z4439">
                  <conditions match="all">
                    <condition match="all" variable="admin-flag"/>
                    <condition match="none" variable="genre container-title"/>
                  </conditions>
                  <date variable="issued" form="text" date-parts="year-month-day"/>
                </else-if>
                <else-if condition-id="z4440">
                  <conditions match="all">
                    <condition match="all" variable="container-title genre authority"/>
                    <condition match="any" variable="volume collection-number"/>
                    <condition match="none" variable="publisher references"/>
                  </conditions>
                  <!-- null -->
                </else-if>
                <else-if condition-id="z4441">
                  <conditions match="all">
                    <condition match="all" variable="admin-flag container-title number page collection-number"/>
                    <condition match="none" variable="title"/>
                  </conditions>
                  <date variable="issued" form="text" date-parts="year-month-day"/>
                </else-if>
                <else-if condition-id="z4442">
                  <conditions match="all">
                    <condition match="all" variable="title container-title authority issued number collection-number admin-flag"/>
                  </conditions>
                  <date variable="issued" form="text" date-parts="year-month-day"/>
                </else-if>
              </choose>
            </if>
            <else-if condition-id="z4443" type="legislation"/>
            <else-if condition-id="z4444" type="regulation">
              <choose>
                <if condition-id="z4445">
                  <conditions match="all">
                    <condition match="all" variable="container-title issued publisher"/>
                    <condition match="none" variable="gazette-flag"/>
                  </conditions>
                  <group delimiter=" ">
                    <text variable="publisher"/>
                    <date form="text" date-parts="year-month-day" variable="issued"/>
                  </group>
                </if>
                <else-if condition-id="z4446" variable="gazette-flag">
                  <date form="text" date-parts="year-month-day" variable="issued"/>
                </else-if>
                <else condition-id="z4447">
                  <group delimiter=" ">
                    <choose>
                      <if condition-id="z4448">
                        <conditions match="all">
                          <condition match="all" variable="authority title"/>
                          <condition match="none" variable="genre"/>
                        </conditions>
                        <names variable="authority">
                          <name/>
                          <institution institution-parts="short" use-last="1"/>
                        </names>
                        <text variable="genre" prefix="[" suffix="]" text-case="capitalize-all"/>
                      </if>
                    </choose>
                  </group>
                  <group delimiter=" ">
                    <choose>
                      <if condition-id="z4449" variable="issued">
                        <choose>
                          <if condition-id="z4450" match="none" variable="title">
                            <text variable="genre"/>
                          </if>
                        </choose>
                        <date form="text" date-parts="year-month-day" variable="issued"/>
                      </if>
                      <else condition-id="z4451">
                        <date form="text" date-parts="year" variable="publication-date"/>
                      </else>
                    </choose>
                  </group>
                </else>
              </choose>
            </else-if>
            <else-if condition-id="z4452" type="patent">
              <group delimiter=") (">
                <group delimiter=" ">
                  <text value="filed"/>
                  <date variable="submitted" form="text" date-parts="year-month-day"/>
                </group>
                <group delimiter=" ">
                  <text value="issued"/>
                  <date variable="issued" form="text" date-parts="year-month-day"/>
                </group>
              </group>
            </else-if>
            <else-if condition-id="z4453" type="report">
              <date form="text" date-parts="year-month-day" variable="issued"/>
            </else-if>
            <else-if condition-id="z4454" type="hearing">
              <choose>
                <if condition-id="z4455" match="none" variable="committee"/>
                <date form="text" date-parts="year-month-day" variable="issued"/>
                <else condition-id="z4456">
                  <date form="text" date-parts="year" variable="issued"/>
                </else>
              </choose>
            </else-if>
          </choose>
        </group>
      </group>
      <group delimiter=" " prefix="(" suffix=")">
        <choose>
          <if condition-id="z4457">
            <conditions match="all">
              <condition match="all" type="legal_case" variable="document-name"/>
              <condition match="any" variable="container-title page"/>
            </conditions>
            <group delimiter=" ">
              <label variable="number" form="short" text-case="capitalize-first"/>
              <number variable="number"/>
            </group>
          </if>
          <else-if condition-id="z4458">
            <conditions match="all">
              <condition match="all" type="legal_case"/>
              <condition match="all" variable="page archive_location"/>
            </conditions>
            <group delimiter=" ">
              <text term="number" form="short" text-case="capitalize-first"/>
              <text variable="archive_location"/>
            </group>
          </else-if>
          <else-if condition-id="z4459">
            <conditions match="all">
              <condition match="all" variable="admin-flag container-title publisher number genre"/>
              <condition match="none" variable="collection-number"/>
            </conditions>
            <text variable="genre"/>
          </else-if>
          <else-if condition-id="z4460" match="none" type="patent">
            <text variable="status"/>
          </else-if>
        </choose>
      </group>
    </group>
  </macro>
  <macro name="juris-pretitle-short">
    <text macro="jurism-pretitle"/>
  </macro>
  <macro name="juris-title-short">
    <text macro="juris-title"/>
  </macro>
  <macro name="juris-main-short">
    <group delimiter=" ">
      <choose>
        <if condition-id="z4493" type="patent">
          <group delimiter=" ">
            <number variable="number" substring="-3" prefix="’"/>
            <text value="Patent"/>
          </group>
        </if>
        <else>
          <text macro="juris-main"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="juris-tail-short">
    <group delimiter=" " prefix="(" suffix=")">
      <choose>
        <if condition-id="z4497">
          <conditions match="all">
            <condition match="all" type="legal_case" variable="document-name"/>
            <condition match="any" variable="container-title page"/>
          </conditions>
          <group delimiter=" ">
            <label variable="number" form="short" text-case="capitalize-first"/>
            <number variable="number"/>
          </group>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="juris-locator">
    <group delimiter=" ">
      <choose>
        <if condition-id="z4498" match="all" type="patent" locator="field">
          <text value="at"/>
          <label variable="locator" form="static"/>
        </if>
        <else-if condition-id="z4499">
          <conditions match="any">
            <condition locator="page" type="report" variable="number" match="all"/>
            <condition locator="page" type="legal_case" variable="archive" match="all"/>
            <condition locator="page" type="legal_case" variable="archive_location" match="all"/>
            <condition type="legal_case" variable="document-name" match="all"/>
          </conditions>
          <text value="at"/>
        </else-if>
        <else-if condition-id="z4500" locator="page" position="ibid-with-locator">
          <text value="at"/>
        </else-if>
        <else-if condition-id="z4501" locator="page" match="any"/>
        <else-if condition-id="z4502">
          <conditions match="all">
            <condition type="regulation legislation" match="any"/>
            <condition locator="rule"/>
          </conditions>
        </else-if>
        <else condition-id="z4503">
          <label variable="locator"/>
        </else>
      </choose>
      <number variable="locator"/>
    </group>
  </macro>
</style>
